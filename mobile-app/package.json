{
  "name": "axs360-mobile",
  "version": "1.0.0",
  "description": "AXS360 Mobile App - Premium Vehicle Access Management",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios", 
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace AXS360.xcworkspace -scheme AXS360 -configuration Release -archivePath AXS360.xcarchive archive",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "bundle:ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle",
    "postinstall": "npx react-native link",
    
    "build:android:release": "cd android && ./gradlew assembleRelease",
    "build:android:debug": "cd android && ./gradlew assembleDebug", 
    "build:android:bundle": "cd android && ./gradlew bundleRelease",
    
    "build:ios:release": "cd ios && xcodebuild -workspace AXS360.xcworkspace -scheme AXS360 -configuration Release clean archive -archivePath build/AXS360.xcarchive",
    "build:ios:debug": "cd ios && xcodebuild -workspace AXS360.xcworkspace -scheme AXS360 -configuration Debug clean build",
    
    "deploy:android:play": "npx @react-native-community/cli upload-to-play",
    "deploy:ios:testflight": "cd ios && xcrun altool --upload-app --file build/AXS360.ipa --username YOUR_APPLE_ID --password YOUR_APP_PASSWORD",
    
    "clean": "npx react-native clean",
    "clean:android": "cd android && ./gradlew clean",
    "clean:ios": "cd ios && xcodebuild clean",
    "clean:node": "rm -rf node_modules && npm install",
    
    "adb:devices": "adb devices",
    "adb:reverse": "adb reverse tcp:8081 tcp:8081",
    "adb:shake": "adb shell input keyevent 82",
    
    "simulator:ios": "npx react-native run-ios --simulator=\"iPhone 15 Pro\"",
    "simulator:android": "npx react-native run-android",
    
    "generate:icons": "npx react-native-asset",
    "generate:splash": "npx react-native-splash-screen generate",
    
    "analyze:android": "cd android && ./gradlew analyzeReleaseBundle",
    "analyze:ios": "cd ios && xcodebuild -workspace AXS360.xcworkspace -scheme AXS360 -configuration Release analyze"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-community/geolocation": "^3.0.6",
    "@react-native-community/netinfo": "^9.4.1",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@reduxjs/toolkit": "^1.9.5",
    "react": "18.2.0",
    "react-native": "0.72.4",
    "react-native-vector-icons": "^9.2.0",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-reanimated": "^3.4.2",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-screens": "^3.24.0",
    "react-native-qrcode-scanner": "^1.5.5",
    "react-native-camera": "^4.2.1",
    "react-native-permissions": "^3.8.4",
    "react-native-biometrics": "^3.0.1",
    "react-native-keychain": "^8.1.2",
    "react-native-push-notification": "^8.1.1",
    "react-native-splash-screen": "^3.3.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-svg": "^13.9.0",
    "react-native-maps": "^1.7.1",
    "react-native-image-picker": "^5.6.0",
    "react-native-document-picker": "^9.0.1",
    "react-native-share": "^9.4.1",
    "react-redux": "^8.1.2",
    "redux-persist": "^6.0.0",
    "axios": "^1.5.0",
    "react-hook-form": "^7.45.4",
    "react-native-modal": "^13.0.1",
    "react-native-toast-message": "^2.1.6",
    "react-native-loading-spinner-overlay": "^3.0.1",
    "lottie-react-native": "^6.2.0",
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^10.8.0",
    "react-native-app-intro-slider": "^4.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.9",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
